<!DOCTYPE html>
<html lang="en">
<head>
    <%- include("head.ejs") %>
</head>
<body>
    <section>
            <div class="container">
                <table class="table">
                    <thead>
                        <th></th>
                        <th>FirstName</th>
                        <th>LastName</th>
                        <th>Mobile</th>
                        <th>User Name</th>
                        <th>Password</th>
                        <th></th>
                    </thead>
                    <tbody>
                        <% products.forEach(e=>{ %>
                            <tr>
                                <td><%= e.FirstName %></td>
                                <td><%= e.LastName %></td>
                                <td><%= e.Mobile %></td>
                                <td><%= e.User Name %></td>
                                <td><%= e.Password %></td>
                                <td>
                                    <a href="#" class="btn btn-outline-primary">Edit</a>
                                </td>
                            </tr>    
                       <% }) %>
                    </tbody>
                </table>
        </div>
    </section>
</body>
</html>